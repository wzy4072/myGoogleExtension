{
    "manifest_version": 2,
    "name": "mydemo",
    "version": "1.0.2",
    "description": "逻辑列队执行",
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "this is AY's extension",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/jq3.3.1.js",
                "js/tools.js",
                "js/content-script.js"
            ],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://192.168.1.21:8033/*"
    ],
    "homepage_url": "https://www.baidu.com"
}